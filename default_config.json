{
    "disassemble": true,
    "objdump": "riscv64-unknown-elf-objdump",
    "data_forwarding": true,
    "branch_predictor": "branch_history_table",
    "stack_size": 1024,
    "alu_cycles": {
        "add_sub": 1,
        "mul": 3,
        "div_rem": 30,
        "bit_op": 1,
        "slt": 1
    },
    "ecall_cycles": {
        "cputchar": 2000,
        "sbrk": 1000,
        "readint": 10000,
        "time": 1000
    },
    "memory_cycles": 100,
    "cache": [
        {
            "name": "L1_instruction_cache",
            "instruction_entry": true,
            "size": 48,
            "associativity": 12,
            "cache_line_bytes": 64,
            "write_back": true,
            "write_allocate": true,
            "hit_cycles": 1,
            "cache_for": "L2_cache"
        },
        {
            "name": "L1_data_cache",
            "data_entry": true,
            "size": 32,
            "associativity": 8,
            "cache_line_bytes": 64,
            "write_back": true,
            "write_allocate": true,
            "hit_cycles": 1,
            "cache_for": "L2_cache"
        },
        {
            "name": "L2_cache",
            "size": 512,
            "associativity": 8,
            "cache_line_bytes": 64,
            "write_back": true,
            "write_allocate": true,
            "hit_cycles": 8,
            "cache_for": "L3_cache"
        },
        {
            "name": "L3_cache",
            "size": 8192,
            "associativity": 16,
            "cache_line_bytes": 64,
            "write_back": true,
            "write_allocate": true,
            "hit_cycles": 20,
            "cache_for": "memory"
        }
    ]
}
